{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "ngrx-traits",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "build": "ng-packagr -p package.json",
      "postinstall": "ngcc"
  },
  "ngPackage": {
    "dest": "../../dist/libs/ngrx-traits",
    "lib": {
      "entryFile": "src/lib/public_api.ts"
    }
  },
  "peerDependencies": {
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@ngrx/effects": "^12.2.0",
    "@ngrx/entity": "^12.2.0",
    "@ngrx/store": "^12.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielguerrero/ngrx-traits.git"
  }
}
